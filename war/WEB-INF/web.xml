<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>smaNotification</servlet-name>
		<servlet-class>com.gerstoft.sma.web.SmaNotificationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>smaNotification</servlet-name>
		<url-pattern>/smanotification</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SmaChart</servlet-name>
		<servlet-class>com.gerstoft.sma.web.SmaChartServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SmaChart</servlet-name>
		<url-pattern>/smachart</url-pattern>
	</servlet-mapping>
		<servlet>
		<servlet-name>Recache</servlet-name>
		<servlet-class>com.gerstoft.sma.web.RecacheServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Recache</servlet-name>
		<url-pattern>/recache</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
